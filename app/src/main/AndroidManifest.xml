<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lola.bookapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon1"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.BookApp">
        <activity android:name=".Ui.GetFavouriteData"></activity>
        <activity android:name=".Ui.Recycler_Love" />
        <activity android:name=".Ui.Recycler_Life" />
        <activity android:name=".Ui.Recycler_Sport" />
        <activity android:name=".View.Book_details" />
        <activity android:name=".Ui.Recycler_Tiem" />
        <activity android:name=".Ui.MainActivity" />
        <activity android:name=".Ui.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>